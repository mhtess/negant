	Issn = {15307026},
	Journal = {Cognitive, Affective and Behavioral Neuroscience},
	Keywords = {Emotion,Emotion regulation,Negation,Startle reflex},
	Mendeley-Groups = {negated-antonyms},
	Number = {2},
	Pages = {199--206},
	Title = {{Negation as a means for emotion regulation? Startle reflex modulation during processing of negated emotional words}},
